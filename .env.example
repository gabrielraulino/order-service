# Order Service - Environment Variables
# Copy this file to .env and update with your actual values

# Server Configuration
SERVER_PORT=8084

# Database Configuration
SPRING_DATASOURCE_URL=jdbc:postgresql://localhost:5435/ms-order
SPRING_DATASOURCE_USERNAME=postgres
SPRING_DATASOURCE_PASSWORD=your-database-password

# RabbitMQ Configuration (CloudAMQP)
# Format: amqps://user:password@host.rmq.cloudamqp.com/vhost
SPRING_RABBITMQ_ADDRESSES=amqps://user:password@host.rmq.cloudamqp.com/vhost

# Eureka Service Discovery
EUREKA_SERVER_URL=http://localhost:8761/eureka/
HOSTNAME=localhost

# JWT Configuration
JWT_SECRET=your-secret-key-change-this-in-production-min-256-bits-for-security
